; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Blood Worm"
run/main_scene="res://Entities/Main.tscn"
config/icon="res://icon.png"

[display]

window/size/width=1200
window/size/height=800
window/size/resizable=false
window/size/test_width=1200
window/size/test_height=800

[layer_names]

2d_physics/layer_1="Mouth"
2d_physics/layer_2="Head"
2d_physics/layer_3="Body"
2d_physics/layer_4="Wall"
2d_physics/layer_5="Enemy"
2d_physics/layer_6="Food"

[rendering]

quality/2d/use_pixel_snap=true
environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"
